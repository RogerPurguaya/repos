                    <field name="month_id" invisible="1"/>
                    <field name="check_period" invisible="1"/>

                    <field name="date"     readonly="1"/>
                    <field name="vale_diesel" attrs="{'readonly':[('check_period','=',True)]}"/>
                    <field name="diesel"   attrs="{'readonly':[('check_period','=',True)]}"/>
                </tree>
            </field>
